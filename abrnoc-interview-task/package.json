    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^10.3.0",
    "jest": "^30.0.4",
    "json-server": "^1.0.0-beta.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
